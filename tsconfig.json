  "compilerOptions": {
    "target": "ESNext",
    "module": "system",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true,
    "outFile": "./dist/IwaraDownloadTool.user.js"
  },
  "include": ["./scripts/*.ts", "./utils/*.d.ts"],
